/* USER CODE END Header */
/* Includes ------------------------------------------------------------------*/
#include "main.h"

/* Private includes ----------------------------------------------------------*/
/* USER CODE BEGIN Includes */
#include "stdio.h"
#include "math.h"
/* USER CODE END Includes */

/* Private typedef -----------------------------------------------------------*/
/* USER CODE BEGIN PTD */

/* USER CODE END PTD */

/* Private define ------------------------------------------------------------*/
/* USER CODE BEGIN PD */

#define SAMPLE_WINDOW 15
#define TAP_P2P_THRESHOLD 0.05f
#define DRAG_VARIANCE_THRESHOLD 0.0002f

#define STATE_IDLE 0x01
#define STATE_TAP 0x02
#define STATE_DRAG 0x03

/* USER CODE END PD */

/* Private macro -------------------------------------------------------------*/
/* USER CODE BEGIN PM */

/* USER CODE END PM */

/* Private variables ---------------------------------------------------------*/
ADC_HandleTypeDef hadc1;

I2C_HandleTypeDef hi2c1;

UART_HandleTypeDef hlpuart1;
UART_HandleTypeDef huart2;

/* USER CODE BEGIN PV */

/* USER CODE END PV */

/* Private function prototypes -----------------------------------------------*/
void SystemClock_Config(void);
static void MX_GPIO_Init(void);
static void MX_I2C1_Init(void);
static void MX_LPUART1_UART_Init(void);
static void MX_USART2_UART_Init(void);
static void MX_ADC1_Init(void);
/* USER CODE BEGIN PFP */

void send_motion_packet(uint8_t motion_state, uint8_t lidar_x, int8_t lidar_y) {
	uint8_t packet[7];
	packet[0] = 0xAA;
	packet[1] = motion_state;
	packet[2] = lidar_x;
	packet[3] = lidar_y;
	packet[4] = 0x00; //reserved for later use


	HAL_UART_Transmit(&huart2, packet, 5, HAL_MAX_DELAY);

}

/* USER CODE END PFP */

/* Private user code ---------------------------------------------------------*/
/* USER CODE BEGIN 0 */
I2C_HandleTypeDef hi2cl;

/* USER CODE END 0 */

/**
  * @brief  The application entry point.
  * @retval int
  */
int main(void)
{

  /* USER CODE BEGIN 1 */

  /* USER CODE END 1 */

  /* MCU Configuration--------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */

  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  MX_I2C1_Init();
  MX_LPUART1_UART_Init();
  MX_USART2_UART_Init();
  MX_ADC1_Init();
  /* USER CODE BEGIN 2 */
  #define SAD_W_M 0x32
  #define SAD_R_M 0x33


  /* USER CODE END 2 */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */

  uint8_t buf[10];


   buf[0] = 0x20;


   buf[1] = 0x97;


   #define SAD_W_M 0x32


   #define SAD_R_M 0x33


   HAL_StatusTypeDef ret = HAL_I2C_Master_Transmit(&hi2c1, SAD_W_M, &buf[0], 2, 1000);


   float accel_buffer[SAMPLE_WINDOW];
   int index = 0;
   int sample_count = 0;


  while (1)
  {

//	  uint32_t ADC_VAL = 0;
//	  HAL_ADC_Start(&hadc1);
//	  HAL_ADC_PollForConversion(&hadc1, 0xFFFFFFFF);
//	  ADC_VAL = HAL_ADC_GetValue(&hadc1);
//	  printf("ADC: %lu \n\r", ADC_VAL);
//
//	  HAL_Delay(20);

	    // Read all 3 axes at once (6 bytes from 0x28 = OUT_X_L_A)
	    uint8_t reg = 0xA8; // auto-increment enabled
	    HAL_I2C_Master_Transmit(&hi2c1, SAD_W_M, &reg, 1, 1000);
	    uint8_t data[6];
	    HAL_I2C_Master_Receive(&hi2c1, SAD_R_M, data, 6, 1000);

	    int16_t x_raw = (int16_t)((data[1] << 8) | data[0]);
	    int16_t y_raw = (int16_t)((data[3] << 8) | data[2]);
	    int16_t z_raw = (int16_t)((data[5] << 8) | data[4]);

	    float x = x_raw * 0.00006104f;
	    float y = y_raw * 0.00006104f;
	    float z = z_raw * 0.00006104f;

	    // Calculate total movement (simplified magnitude)
	    float magnitude = fabsf(x) + fabsf(y) + fabsf(z);

	    // Save to buffer
	    accel_buffer[index] = magnitude;
	    index = (index + 1) % SAMPLE_WINDOW;
	    sample_count++;

	    // Default to IDLE
	    uint8_t motion_state = STATE_IDLE;

	    if (sample_count >= SAMPLE_WINDOW)
	    {
	        float mean = 0.0f;
	        float max = accel_buffer[0];
	        float min = accel_buffer[0];

	        for (int i = 0; i < SAMPLE_WINDOW; i++)
	        {
	            float val = accel_buffer[i];
	            mean += val;
	            if (val > max) max = val;
	            if (val < min) min = val;
	        }
	        mean /= SAMPLE_WINDOW;

	        float variance = 0.0f;
	        for (int i = 0; i < SAMPLE_WINDOW; i++)
	        {
	            float diff = accel_buffer[i] - mean;
	            variance += diff * diff;
	        }
	        variance /= SAMPLE_WINDOW;

	        float peak_to_peak = max - min;

	        // Motion classification
	        if (peak_to_peak > TAP_P2P_THRESHOLD && variance > 0.00001f)
	        {
	            printf("TAP detected! P2P=%.4f, Var=%.6f\n\r", peak_to_peak, variance);
	            motion_state = STATE_TAP;
	        }
	        else if (variance > DRAG_VARIANCE_THRESHOLD)
	        {
	            printf("DRAG detected. Var=%.6f\n\r", variance);
	            motion_state = STATE_DRAG;
	        }
	        else
	        {
	            printf("IDLE. Var=%.6f | P2P=%.4f\n\r", variance, peak_to_peak);
	            motion_state = STATE_IDLE;
	        }

	        sample_count = 0;
	    }

	    uint8_t lidar_x = 300;
	    uint8_t lidar_y = 150;
	    motion_state = STATE_TAP;
	    send_motion_packet(motion_state, lidar_x, lidar_y);

	    HAL_Delay(2); // ~500 Hz update





    /* USER CODE END WHILE */

    /* USER CODE BEGIN 3 */
  }
  /* USER CODE END 3 */
}

/**
  * @brief System Clock Configuration
  * @retval None
  */
void SystemClock_Config(void)
{
  RCC_OscInitTypeDef RCC_OscInitStruct = {0};
  RCC_ClkInitTypeDef RCC_ClkInitStruct = {0};

  /** Configure the main internal regulator output voltage
  */
  if (HAL_PWREx_ControlVoltageScaling(PWR_REGULATOR_VOLTAGE_SCALE1) != HAL_OK)
  {
    Error_Handler();
  }

  /** Initializes the RCC Oscillators according to the specified parameters
  * in the RCC_OscInitTypeDef structure.
  */
  RCC_OscInitStruct.OscillatorType = RCC_OSCILLATORTYPE_MSI;
  RCC_OscInitStruct.MSIState = RCC_MSI_ON;
  RCC_OscInitStruct.MSICalibrationValue = 0;
  RCC_OscInitStruct.MSIClockRange = RCC_MSIRANGE_6;
  RCC_OscInitStruct.PLL.PLLState = RCC_PLL_NONE;
  if (HAL_RCC_OscConfig(&RCC_OscInitStruct) != HAL_OK)
  {
    Error_Handler();
  }

  /** Initializes the CPU, AHB and APB buses clocks
  */
  RCC_ClkInitStruct.ClockType = RCC_CLOCKTYPE_HCLK|RCC_CLOCKTYPE_SYSCLK
                              |RCC_CLOCKTYPE_PCLK1|RCC_CLOCKTYPE_PCLK2;
  RCC_ClkInitStruct.SYSCLKSource = RCC_SYSCLKSOURCE_MSI;
  RCC_ClkInitStruct.AHBCLKDivider = RCC_SYSCLK_DIV1;
  RCC_ClkInitStruct.APB1CLKDivider = RCC_HCLK_DIV1;
  RCC_ClkInitStruct.APB2CLKDivider = RCC_HCLK_DIV1;

  if (HAL_RCC_ClockConfig(&RCC_ClkInitStruct, FLASH_LATENCY_0) != HAL_OK)
  {
    Error_Handler();
  }
}

